{
    "name": "website",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "tsc && pnpx next lint && pnpm run prettier",
        "format": "eslint --fix ./**/*.{ts,tsx,js,css} && prettier --write ./**/*.{ts,tsx,js,scss,css,md,json}",
        "test": "pnpm test:e | pnpm test:c",
        "test:c": "cypress run --headed --component",
        "test:e": "cypress run --headed",
        "test:h": "pnpm test:headless",
        "test:headless": "pnpm test:headless:e | pnpm test:headless:c",
        "test:headless:c": "cypress run --component",
        "test:headless:e": "cypress run",
        "prettier": "prettier --check ./**/*.{ts,tsx,js,scss,css,md,json}"
    },
    "dependencies": {
        "next": "13.0.6",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-draggable": "^4.4.5",
        "react-rnd": "^10.4.1"
    },
    "devDependencies": {
        "@types/node": "18.11.13",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "@typescript-eslint/parser": "^5.45.1",
        "autoprefixer": "^10.4.13",
        "csstype": "^3.1.1",
        "cypress": "^12.2.0",
        "eslint": "8.29.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-next": "^13.0.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "postcss": "^8.4.19",
        "prettier": "^2.8.1",
        "sass": "^1.56.2",
        "tailwindcss": "^3.2.4",
        "typescript": "4.9.4"
    }
}
